Client.prototype.known_method = function(foo, bar, baz)
{
    if (foo != 'foo')
        throw new Error('Unexpected arguments');

    if (bar != 'bar')
        throw new Error('Unexpected arguments');

    if (baz != ['baz'])
        throw new Error('Unexpected arguments');

    this.call('client_method', null, 1, 2, 3);

    return 'ok';
}


application.onConnect = function( p_client, p_autoSenseBW )
{
	return true;
}
